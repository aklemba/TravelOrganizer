<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ekipaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MenuActivity"
            android:label="@string/menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.location.LocationsActivity"/>
        <activity android:name=".ui.location.AddLocationActivity" />
        <activity android:name=".ui.location.EditLocationActivity" />
        <activity
            android:name=".ui.auth.LoginActivity"
            android:label="@string/loginLabel"/>
        <activity
            android:name=".ui.auth.RegistrationActivity"
            android:label="@string/registrationLabel"/>
        <activity
            android:name=".ui.event.EventsActivity"
            android:label="@string/events"/>
        <activity android:name=".ui.event.AddEventActivity" />
    </application>
</manifest>
