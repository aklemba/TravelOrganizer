<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ekipaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LocationsActivity"/>
        <activity android:name=".ui.AddLocationActivity" />
        <activity android:name=".ui.EditLocationActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.RegistrationActivity" />
    </application>
</manifest>
